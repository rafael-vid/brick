    <script>
        document.getElementById('minimize-button').addEventListener('click', function () {
            var menu = document.getElementById('menu-dashboard');
            if (menu.style.width === '50%') {
                menu.style.width = '100%';
            } else {
                menu.style.width = '50%';
            }
        });
        function handleResize() {
            var menu = document.getElementById('menu-dashboard');
