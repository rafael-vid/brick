    <script>
        function handleResize() {
           const section = document.querySelector('.app') || document.querySelector('.mini-menu');
           if (window.innerWidth < 758) {
            section.className = 'app mini-menu';
           } 
           else {
            section.className = 'app';
           }
        }

        window.addEventListener('resize', handleResize);
        document.addEventListener('DOMContentLoaded', handleResize);
    </script>
    <script>
        function handleDynamicTextVisibility() {
            const dynamicText = document.getElementById('dynamic-text');
            if (window.innerWidth < 758) {
                if (dynamicText) {
                    dynamicText.style.display = 'none';
                }
            } else {
                if (dynamicText) {
                    dynamicText.style.display = '';
                }
            }
        }

        // Chame a função no evento de redimensionamento e quando o DOM for carregado
        window.addEventListener('resize', handleDynamicTextVisibility);
        document.addEventListener('DOMContentLoaded', handleDynamicTextVisibility);
    </script>
    <script>
        function handleDynamicTextVisibility2() {
            const dynamicText = document.getElementById('dynamic-textmobile');
            if (window.innerWidth > 758) {
                if (dynamicText) {
                    dynamicText.style.display = 'none';
                }
            } else {
                if (dynamicText) {
                    dynamicText.style.display = 'flex';
                }
            }
        }

        // Chame a função no evento de redimensionamento e quando o DOM for carregado
        window.addEventListener('resize', handleDynamicTextVisibility2);
        document.addEventListener('DOMContentLoaded', handleDynamicTextVisibility2);
    </script>
    <script>
        document.addEventListener('DOMContentLoaded', () => {
            const btnMobile = document.getElementById('btn-mobile');
            const nav = document.getElementById('nav');

            btnMobile.addEventListener('click', () => {
                nav.classList.toggle('active');
            });

            const dropdowns = document.querySelectorAll('[data-dropdow-perfil]');
            dropdowns.forEach((dropdown) => {
                dropdown.addEventListener('click', function (event) {
                    const submenu = this.querySelector('.dropdown-perfil');
                    if (submenu) {
                        submenu.style.display = submenu.style.display === 'block' ? 'none' : 'block';
                        event.stopPropagation(); // Evita fechar outros menus ao clicar
                    }
                });

                document.addEventListener('click', () => {
                    dropdown.querySelector('.dropdown-perfil').style.display = 'none';
                });
            });
        });
    </script>
